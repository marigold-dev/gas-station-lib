{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "examples"
  ]
}
